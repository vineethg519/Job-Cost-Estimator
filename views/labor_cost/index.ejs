<% console.log(estimatePartLabors.query); %>
<div class="container">
 <h2>All entries</h2>

<p>There are  <%= estimatePartLabors.query[0].entries.length %> entries. <br/>

				<a href="estimatePartLabor/create"><button type="button" class="btn btn-info">Add a new one.</button></a></p>

<table class="table" border="1">
    <thead>
      <tr>
       <th>ID</th>
       <th>type</th>
       <th>count</th>
       <th>hoursPerPerson</th>
       <th>dollarsPerHour</th>
       <th>nightsPerPerson</th>
       <th>costPerNight</th>

       <th>Actions</th>
    </tr>
    </thead>
    <tbody>
<% for (var i = 0; i < estimatePartLabors.query.length; i++) { 
     for(var j=0; j<estimatePartLabors.query[i].entries.length;j++){
        var entry = estimatePartLabors.query[i].entries[j];
           
      %>
        <tr>
            <td> <%= entry._id %></td>
            <td> <%= entry.type %></td>
            <td> <%= entry.count %></td>
            <td> <%= entry.hoursPerPerson %>
            <td> <%= entry.dollarsPerHour %></td>
            <td> <%= entry.nightsPerPerson %></td>
            <td> <%= entry.costPerNight %></td></td>
             
             
            <td>
            <a href="estimatePartLabor/delete/<%=  entry._id %>"> <button type="button" class="btn btn-danger"> Delete </button> </a> 
							<a href="estimatePartLabor/details/<%= entry._id %>"> <button type="button" class="btn btn-info"> Details </button> </a> 
							<a href="estimatePartLabor/edit/<%=    entry._id %>"> <button type="button" class="btn btn-warning"> Edit </button> </a>
            </td>
         </tr>
         <% } %>
         <% } %>
        
         
</tbody>
</table>
</div>

</tbody>
</table>
</div>
